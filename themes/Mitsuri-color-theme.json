				"constant.numeric",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other",
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#9D7DCE"
			}
		},
		{
			"name": "String, Symbols, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#F99157"
			}
		},
		{
			"name": "Class, Support",
